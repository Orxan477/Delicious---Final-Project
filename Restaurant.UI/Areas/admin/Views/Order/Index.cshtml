@model List<FullOrder>
@{
    ViewData["Title"] = "Index";
    int n = 1;
}

<div class="row">
    <div class="col-lg-12">
        <table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">FullName</th>
            <th scope="col">Order Date</th>
            <th scope="col">Total Price</th>
            <th scope="col">Setting</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
       {
           <tr>
            <th scope="row">@n</th>
            <td>@item.BillingAdress.AppUser.FullName</td>
            <td>@item.CreatedAt</td>
            <td>@item.Total</td>
            <td><a class="btn btn-light" asp-action="Detail" asp-route-id="@item.Id">Detail</a></td>
        </tr>
        n++;
       }
    </tbody>
</table>
    </div>
</div>

